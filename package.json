{
	"name": "xml-paste",
	"displayName": "XML Paste",
	"description": "Paste escaped XML or HTML into a file. It simply replaces angle brackets and ampersands with HTML entities.",
	"version": "0.5.3",
	"publisher": "Webucator",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Formatters",
		"Other"
	],
	"keywords": [
		"copy",
		"paste",
		"xml",
		"html"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/WebucatorTraining/vscode-paste-xml"
	},
	"icon": "images/icon-128.png",
	"activationEvents": [
		"onCommand:extension.xmlPaste"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.xmlPaste",
				"title": "XML Paste"
			},
			{
				"command": "extension.xmlPasteUnescaped",
				"title": "XML Paste Unescaped"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^13.7.0",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^10.2.0",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {}
}
